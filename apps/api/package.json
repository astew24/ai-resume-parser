{
  "name": "ai-resume-parser-api",
  "version": "1.0.0",
  "description": "AI Resume Parser API - Express.js backend for parsing resumes with AI",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node --watch src src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
